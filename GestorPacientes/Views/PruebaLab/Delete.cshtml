@using GestorPacientes.Core.Application.ViewModels.PruebaLab
@model PruebaLabUpdateViewModel

@{
    ViewData["Title"] = "PruebaLab Eliminando";
}

<div class="card">
    <div class="card-body">
        <h5> @ViewData["Title"]</h5>
        <hr />
        <h3>¿Quieres eliminar esta prueba?</h3>
        <dl class="row md-2 mt-3">
            <dt class="col-sm-2">
                Nombre
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NombrePruebaLab)
            </dd>
        </dl>
        <form asp-action="Delete" class="row justify-content-center">
            <button type="submit" class="btn btn-danger col-2 mx-2">Aceptar</button>
            <a asp-action="Index" class="btn btn-primary col-2 mx-2">Cancelar</a>
        </form>
    </div>
</div>